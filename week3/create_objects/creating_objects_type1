    // Using Function as a Class. 
    function copyClass(name, age) { 
        this.name = name; 
        this.age = age; 
        this.printInfo = function() { 
            console.log(this.name); 
            console.log(this.age); 
        } 
    } 
  
// Creating the object of copyClass 
// and initializing the parameters. 
var obj = new copyClass("Vaishu", 20); 
var obj1 = new copyClass("Manu", 21); 
// Calling the method of copyClass. 
obj.printInfo(); 
obj1.printInfo(); 

